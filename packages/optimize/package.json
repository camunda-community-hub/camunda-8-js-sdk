{
	"name": "@camunda8/optimize",
	"version": "8.4.0",
	"description": "A Node.js client for interacting with the Camunda 8 Optimize API",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest --detectOpenHandles --verbose --testPathIgnorePatterns integration",
		"test:integration": "jest --verbose --detectOpenHandles",
		"test:local-integration": "jest --runInBand --detectOpenHandles --verbose true",
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
		"compile": "tsc",
		"docs": "typedoc src/index.ts",
		"prettify": "prettier --write 'src/**/*.ts'",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/camunda-community-hub/camunda-8-js-sdk.git"
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"Camunda"
	],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/camunda-community-hub/camunda-8-js-sdk/issues"
	},
	"homepage": "https://github.com/camunda-community-hub/camunda-8-js-sdk#readme",
	"devDependencies": {
		"typedoc": "^0.23.21"
	},
	"dependencies": {
		"@camunda8/oauth": "^8.4.0",
		"debug": "^4.3.4",
		"got": "^11.8.5"
	},
	"gitHead": "45b1f93347bbe930d5782287d6821efec95e9a63"
}