{
	"name": "@camunda8/console",
	"version": "8.4.0",
	"description": "A Node.js Camunda 8 Web Console API client",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest --detectOpenHandles  --verbose true --testPathIgnorePatterns integration",
		"test:integration": "jest --runInBand --detectOpenHandles --verbose true",
		"test:local-integration": "exit 0",
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
		"compile": "tsc",
		"docs": "typedoc src/index.ts",
		"prettify": "prettier --write 'src/**/*.ts'",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/camunda-community-hub/camunda-8-js-sdk.git"
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/camunda-community-hub/camunda-8-js-sdk/issues"
	},
	"homepage": "https://camunda-community-hub.github.io/camunda-8-js-sdk/",
	"devDependencies": {
		"typedoc": "^0.23.28"
	},
	"dependencies": {
		"@camunda8/oauth": "^8.4.0",
		"debug": "^4.3.4",
		"dotenv": "^16.3.1",
		"got": "^11.8.6"
	},
	"jest": {
		"moduleNameMapper": {
			"^@camunda8/oauth(.*)$": "<rootDir>/../oauth/src$1"
		},
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"gitHead": "45b1f93347bbe930d5782287d6821efec95e9a63"
}
