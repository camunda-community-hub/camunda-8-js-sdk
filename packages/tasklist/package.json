{
    "name": "@camunda8/tasklist",
    "version": "8.4.0",
    "description": "A Camunda 8 Tasklist API client for Node.js",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest --detectOpenHandles  --runInBand --verbose --testPathIgnorePatterns integration",
        "test:integration": "jest --detectOpenHandles  --runInBand --verbose",
        "test:local-integration": "jest --runInBand --detectOpenHandles --verbose true",
        "build": "npm run clean && npm run compile",
        "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
        "compile": "tsc",
        "docs": "typedoc --options typedoc.json src/index.ts",
        "prettify": "prettier --write 'src/**/*.ts'",
        "lint": "eslint . --ext .ts,.tsx"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/camunda-community-hub/camunda-8-js-sdk.git"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "Camunda",
        "Tasklist"
    ],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/camunda-community-hub/camunda-8-js-sdk/issues"
    },
    "homepage": "https://github.com/camunda-community-hub/camunda-8-js-sdk#readme",
    "devDependencies": {
        "@camunda8/zeebe": "^8.4.0",
        "typedoc": "^0.23.25"
    },
    "dependencies": {
        "@camunda8/oauth": "^8.4.0",
        "@types/debug": "^4.1.12",
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "got": "^11.8.6"
    },
    "jest": {
        "moduleNameMapper": {
            "^@camunda8/oauth(.*)$": "<rootDir>/../oauth/src$1"
        },
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "gitHead": "45b1f93347bbe930d5782287d6821efec95e9a63"
}
